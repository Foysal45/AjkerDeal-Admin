<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_margin="20dp"
    android:layout_height="wrap_content"
    android:padding="10dp">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/complainHistoryBtn"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="Complain History"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/complainBtn"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="Complain"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/complainHistoryBtn"
        app:layout_constraintStart_toStartOf="@+id/complainHistoryBtn"
        app:layout_constraintTop_toBottomOf="@+id/complainHistoryBtn" />

</androidx.constraintlayout.widget.ConstraintLayout>